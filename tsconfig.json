{
  "compilerOptions": {
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "strict": true,
    "target": "es2020",
    "lib": [
      "es5",
      "dom",
      "dom.iterable",
      "es2019",
      "es2020"
    ],
    "jsx": "react",
    "isolatedModules": false,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "types": ["node"]
  },
  "include": ["**/*.ts", "blah.js"],
  "exclude": ["node_modules"]
}
